{
    "name": "ackly/x-php-health",
    "description": "Health checks for native php7.0+ applications",
    "type": "library",
    "authors": [
        {
            "name": "Andrey Stroganov",
            "email": "a.stroganov@vsemayki.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ackly\\Health\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Mocks\\": "tests/mocks"
        }
    },
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7",
        "phpunit/php-code-coverage": "^6.0"
    }
}
